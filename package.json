{
  "name": "l-hooks",
  "version": "0.0.35",
  "description": "A react hooks library developed with dumi",
  "keywords": [
    "react",
    "hooks",
    "lhooks"
  ],
  "homepage": "https://github.com/Peek-A-Booo/LHooks#readme",
  "bugs": {
    "url": "https://github.com/Peek-A-Booo/LHooks/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Peek-A-Booo/LHooks.git"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "unpkg": "dist/l-hooks.min.js",
  "module": "./es/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "deploy": "gh-pages -d dist",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "pub:docs": "dumi build && gh-pages -d dist",
    "pub:hooks": "npm publish"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.21.0"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "1.6.2",
    "@ant-design/icons": "5.0.1",
    "@babel/core": "7.21.3",
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@emotion/react": "^11.10.6",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@umijs/lint": "4.0.62",
    "antd": "5.3.2",
    "dumi": "2.1.17",
    "dumi-theme-antd-style": "0.19.4",
    "eslint": "8.36.0",
    "father": "4.1.7",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "l-hooks": "0.0.34",
    "lint-staged": "13.2.0",
    "postcss": "8.4.21",
    "prettier": "2.8.6",
    "prettier-plugin-organize-imports": "3.2.2",
    "prettier-plugin-packagejson": "2.4.3",
    "qs": "6.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "styled-components": "5.3.9",
    "stylelint": "15.3.0",
    "typescript": "5.0.2",
    "webpack": "5.75.0"
  },
  "peerDependencies": {
    "@types/qs": ">=6.9.7",
    "qs": ">=6.11.1",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "lsx"
  ]
}
